{
  "name": "pollen-solutions/routing",
  "description": "Pollen Solutions - Routing Component - HTTP request mapping and HTTP response resolution.",
  "homepage": "https://www.presstify.com/pollen-solutions/routing/",
  "keywords": [
    "pollen-solutions",
    "component",
    "PSR-7",
    "PSR-15"
  ],
  "time": "2021-07-09",
  "license": "MIT",
  "authors": [
    {
      "name": "Jordy Manner",
      "email": "jordy@presstify.com"
    }
  ],
  "require": {
    "php": "^7.4 || ^8.0",
    "ext-json": "*",
    "laminas/laminas-diactoros": "^2.5",
    "laminas/laminas-httphandlerrunner": "^1.3",
    "league/route": "^5.0",
    "nyholm/psr7": "^1.3",
    "pollen-solutions/http": "^1.0",
    "pollen-solutions/support": "^1.0",
    "pollen-solutions/view": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "roave/security-advisories": "dev-latest"
  },
  "autoload": {
    "psr-4": {
      "Pollen\\Routing\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pollen\\Routing\\Test\\": "tests"
    }
  },
  "suggest": {
    "pollen-solutions/container" : "Pollen Container Component - PSR-11 ready Dependencies Injection Container."
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}
